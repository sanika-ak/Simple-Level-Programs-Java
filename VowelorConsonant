import java.util.Scanner; 
public class Main { 
public static void main(String[] args) { 

Scanner sc new Scanner(System.in); 
System.out.println("Enter: "); 
String s sc.next();

// checking vowel or consonant using if..else conditional statements
if (s=='a'|'e'|'i'|'o'|'u'|'A'|'E'|'I'|'O'|'U') //stating all Vowels in lower and upper cases
        {
            System.out.println("Vowel");
        }
else
        {
            System.out.println("Consonant");
        }
}
}
